{% spaceless %}
<table>
    {%- if header -%}
        <thead>
        <tr>
            {%- for column in header -%}
                <th>{{ column }}</th>
            {%- endfor -%}
        </tr>
        </thead>
    {%- endif -%}
    <tbody>
    {%- for data in data_set -%}
        <tr>
            {%- for col in data -%}
                <td>{{ col }}</td>
            {%- endfor -%}
        </tr>
    {%- endfor -%}
    </tbody>
</table>
{% endspaceless %}